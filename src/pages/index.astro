---
import BaseLayout from '../layouts/BaseLayout.astro';
import Intro from '../components/sections/Intro.astro';
import App from '../components/sections/App.astro';
import MoneyTransfering from '../components/sections/MoneyTransfering.astro';
import Organizer from '../components/sections/Organizer.astro';
import Notifications from '../components/sections/Notifications.astro';
import Integrations from '../components/sections/Integrations.astro';
import Cards from '../components/sections/Cards.astro';
import Testimonials from '../components/sections/Testimonials.astro';
import AppBanner from '../components/sections/AppBanner.astro';
import Help from '../components/sections/Help.astro';
import Blog from '../components/sections/Blog.astro';
