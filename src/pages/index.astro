---
import BaseLayout from '../layouts/BaseLayout.astro';
import HomepageIntro from '../components/intro/HomepageIntro.astro';
import App from '../components/app/App.astro';
import TransactionsGreen from '../components/transactions/TransactionsGreen.astro';
import Organizer from '../components/savings/Organizer.astro';
import Notifications from '../components/notifications/Notifications.astro';
import Integrations from '../components/tools/Integrations.astro';
import AccountsPerfect from '../components/accounts/AccountsPerfect.astro';
import Testimonials from '../components/./testimonials/Testimonials.astro';
import AppBanner from '../components/app/AppBanner.astro';
import Help from '../components/help/Help.astro';
import Blog from '../components/blog/Blog.astro';

const title = "Homepage";
const scriptsrc = "https://identity.netlify.com/v1/netlify-identity-widget.js";
---

<BaseLayout title={title} scriptsrc={scriptsrc}>
	<HomepageIntro/>
	<App/>
	<TransactionsGreen/>
	<Organizer/>
	<Notifications/>
	<Integrations/>
	<AccountsPerfect/>
	<Testimonials/>
	<AppBanner/>
	<Help/>
	<Blog/>
</BaseLayout>


